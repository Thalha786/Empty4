package Sample1;

import org.testng.Assert;
import org.testng.annotations.Test;
import org.testng.asserts.SoftAssert;

class Sample1
{
	String abc="sample";
	String def="sample1";
	@Test
	public void tes()
	{
		if(abc.equals("sample"))
		{
		SoftAssert softassert=new SoftAssert();
		softassert.assertEquals(abc, "sample");
		}
	
	}
	
	@Test
	public void tes1()
	{
		if(def.equals("sample1"))
		{
			Assert.assertEquals(def, "sdvjsd");
		}
	
	}
}